"github_com/devopsfaith/krakend-cors": {
  "allow_origins": [
    "*"
  ],
  "allow_methods": [
    "HEAD",
    "GET",
    "POST",
    "PUT",
    "PATCH",
    "DELETE",
    "OPTIONS"
  ],
  "expose_headers": [
    "Content-Length",
    "Content-Type"
  ],
  "max_age": "12h",
  "allow_headers": [
    "Accept-Language",
    "Origin",
    "Content-Length",
    "Content-Type",
    "Authorization"
  ],
  "allow_credentials": false,
  "debug": false
}
